{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sbitdatafactoryhuilu001"
		},
		"to_gen2_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'to_gen2_linked_service'"
		},
		"from_github_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/dakuikui001/SBIT_ETL_with_Terraform_on_Azure/refs/heads/main/raw_data/"
		},
		"to_gen2_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dataprojectsforhuilu001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sbitdatafactoryhuilu001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"to_gen2_linked_service_accountKey": {
						"value": "[parameters('to_gen2_linked_service_accountKey')]"
					},
					"from_github_linked_service_properties_typeProperties_url": {
						"value": "[parameters('from_github_linked_service_properties_typeProperties_url')]"
					},
					"to_gen2_linked_service_properties_typeProperties_url": {
						"value": "[parameters('to_gen2_linked_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}