{
	"name": "csvToGen2",
	"properties": {
		"linkedServiceName": {
			"referenceName": "to_gen2_linked_service",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileName": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().fileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(\n    'data_zone/raw/', \n    substring(dataset().fileName, add(indexOf(dataset().fileName, '-'), 1), sub(lastIndexOf(dataset().fileName, '_'), add(indexOf(dataset().fileName, '-'), 1))), \n    '_bz'\n)",
					"type": "Expression"
				},
				"fileSystem": "sbit-project"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}